{
  "name": "vscode-happy-css-modules",
  "version": "0.0.1",
  "displayName": "vscode-happy-css-modules",
  "description": "The vscode extension to improve the development experience for CSS Modules.",
  "type": "commonjs",
  "main": "./out/extension.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vscode-happy-css-modules.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "engines": {
    "vscode": "^1.77.0"
  }
}
